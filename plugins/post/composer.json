{
    "name": "leo/post",
    "type": "library",
    "license": "MIT",
    "autoload": {
       "psr-4": {
            "Leo\\Post\\": "src/",
            "Leo\\Post\\Providers\\":"src/Providers/",
            "Leo\\Post\\Database\\Migrations\\":"database/migrations/",
            "Leo\\Post\\Database\\Seeders\\":"database/seeders/"
        },
        "classmap": [
            "src/Controllers",
            "src/Providers",
            "src/Models",
            "./routes"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Leo\\Post\\Providers\\PostServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "Dom",
            "email": "leodomsolar@gmail.com"
        }
    ],
   "repositories": [],
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
